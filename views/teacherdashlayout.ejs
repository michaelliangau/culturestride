    <script>
        const tSlug = document.getElementById("tSlug").innerHTML;

        // SOCKET FOR NOTIF
        const socket = io({
            transports: ['websocket', 'polling']
        });
        var slug = tSlug;
        socket.on('connect', function () {
            socket.emit('join notif channel', slug);
        });
        socket.on("notif unread message", function (data) {
            console.log("file: teacherdashlayout.ejs ~ line 422 ~ data", data)
            // enable and update red dot in messages side bar
            var dots = document.getElementsByClassName("unreadMsgDot");
            var msgCount = document.getElementById("unreadMsgDot").innerHTML;
            msgCount++;

            for (var i = 0; i < dots.length; i++) {
                dots[i].style.display = "inline-block";
                dots[i].innerHTML = msgCount;
            }
            ajaxUpdateUnreadMsgCount(msgCount);

            // enable and update red dots in messages middle bar if user is on sohomessages
            try {
                document.getElementById("unreadMsgDot" + data).style.display = 'inline-block';
                var count = document.getElementById("unreadMsgDot" + data).innerHTML;
                count++;
                document.getElementById("unreadMsgDot" + data).innerHTML = count;
            } catch (e) {

            }
        });

        function ajaxUpdateUnreadMsgCount(msgCount) { // ajax for update session var so that custom mark view is saved
            $.ajax({
                type: "POST",
                url: "/ajaxUpdateUnreadMsgCount",
                timeout: 30000,
                data: {
                    msgCount: msgCount,
                    who: 'tea',
                },
                success: function (data) {
                    //show content
                    console.log('successful post');
                },
                error: function (jqXHR, textStatus, err) {
                    //show error message
                    console.log('unsuccessful post');
                }
            });
        }

        //* Loop through all dropdown buttons to toggle between hiding and showing its dropdown content - This allows the user to have multiple dropdowns without any conflict */
        var dropdown = document.getElementsByClassName("dropdown-btn");
        var i;

        for (i = 0; i < dropdown.length; i++) {
            dropdown[i].addEventListener("click", function () {
                this.classList.toggle("active");
                var dropdownContent = this.nextElementSibling;
                if (dropdownContent.style.display === "block") {
                    dropdownContent.style.display = "none";
                } else {
                    dropdownContent.style.display = "block";
                }
            });
        }

        // COLLAPSE SIDEBAR
        function collapseSidebar(toggle) {
            if (toggle == 'on') {
                $('#mobileSideBar').css("maxWidth", "90px"); // shrink sidebar              
                $('.desktopTextSideBar').css('display','none');
                $('.desktopTextTopSideBar').css('display','none');
            } else {
                $('#mobileSideBar').css("maxWidth", "16.67%"); // return sidebar to normal width  
                $('.desktopTextSideBar').css('display','');
                $('.desktopTextTopSideBar').css('display','');
            }
        }

        function showSideBar() {
            $('#mobileSideBar').css("maxWidth", "16.67%"); // return sidebar to normal width  
            $('.desktopTextSideBar').show();
            $('.desktopTextTopSideBar').show();
        }


    </script>
