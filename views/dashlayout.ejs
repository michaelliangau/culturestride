    <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.3.1/socket.io.js"></script>

    <script>
        var sSlug = document.getElementById("sSlug").innerHTML;
        // SOCKET FOR NOTIF
        const socket = io({
            transports: ['websocket', 'polling']
        });
        var slug = sSlug;
        socket.on('connect', function () {
            socket.emit('join notif channel', slug);
        });
        socket.on("notif unread message", function (data) {
            var msgCount = document.getElementById("unreadMsgDot").innerHTML;
            // enable red dot in messages side bar
            var dots = document.getElementsByClassName("unreadMsgDot");
            msgCount++;
            for (var i = 0; i < dots.length; i++) { // we use this to update mobile and desktop
                dots[i].style.display = "inline-block";
                dots[i].innerHTML = msgCount;
            }
            // enable and update red dots in messages middle bar (channels) if user is on sohomessages
            try {
                document.getElementById("unreadMsgDot" + data).style.display = 'inline-block';
                var count = document.getElementById("unreadMsgDot" + data).innerHTML;
                count++;
                document.getElementById("unreadMsgDot" + data).innerHTML = count;
            } catch (e) {}
            ajaxUpdateUnreadMsgCount(msgCount);
        });

        function ajaxUpdateUnreadMsgCount(msgCount) { // ajax for update session var so that custom mark view is saved
            $.ajax({
                type: "POST",
                url: "/ajaxUpdateUnreadMsgCount",
                timeout: 30000,
                data: {
                    msgCount: msgCount,
                    who: 'stu',
                },
                success: function (data) {
                    //show content
                    console.log('successful post');
                },
                error: function (jqXHR, textStatus, err) {
                    //show error message
                    console.log('unsuccessful post');
                }
            });
        }
        /* Toggle between showing and hiding the navigation menu links when the user clicks on the hamburger menu / bar icon */
        function mobileSideBar() {
            var x = document.getElementById("mobileSideBar");
            if (x.style.display === "block") {
                x.style.display = "none";
            } else {
                x.style.display = "block";
            }
        }
        //* Loop through all dropdown buttons to toggle between hiding and showing its dropdown content - This allows the user to have multiple dropdowns without any conflict */
        var dropdown = document.getElementsByClassName("dropdown-btn");
        var i;
        for (i = 0; i < dropdown.length; i++) {
            dropdown[i].addEventListener("click", function () {
                this.classList.toggle("active");
                var dropdownContent = this.nextElementSibling;
                if (dropdownContent.style.display === "block") {
                    dropdownContent.style.display = "none";
                } else {
                    dropdownContent.style.display = "block";
                }
            });
        }

        function showSideBar() {
            $('#mobileSideBar').css("maxWidth", "16.67%"); // return sidebar to normal width  
            $('.desktopTextSideBar').show();
            $('.desktopTextTopSideBar').show();
        }

        // GOOGLE SIGN IN
        function onSignIn(googleUser) {
            console.log('google Auth successful')
            var profile = googleUser.getBasicProfile();
            gapi.auth2.getAuthInstance().disconnect();

            // add data to req.body properties
            document.getElementById('googleProfileImage').value = profile.getImageUrl();

            // post form
            document.forms["uploadProfileImageGoogle"].submit();
        }

        // REFER SMOKESCREEN TEST
        function ajaxReferBtnClicked() { // ajax for update session var so that custom mark view is saved
            $.ajax({
                type: "POST",
                url: "/ajaxReferBtnClicked",
                timeout: 30000,
                success: function (data) {
                    //show content
                    console.log('successful post');
                },
                error: function (jqXHR, textStatus, err) {
                    //show error message
                    console.log('unsuccessful post');
                }
            });
        }


        // join next meeting time
        var nextMeetingTime = document.getElementById('sNextMeetingTime').textContent;
        var nextMeetingTimeMom = moment(nextMeetingTime, 'YYYY-MM-DDTHH:mm:ssZ').local().format('ddd, MMM DD LT');
        try {
        document.getElementById('joinNextMeetingTime').textContent = nextMeetingTimeMom;
        } catch (e) {
        }
    </script>
