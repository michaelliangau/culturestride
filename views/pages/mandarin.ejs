<script>
	$(function () {
		$('[data-toggle="tooltip"]').tooltip({
			boundary: 'window'
		})
	});

	// TOGGLE SHOW HSK cards
	function toggleHSKCards() {
		if ($('#lineHSK3').css('display') == 'none') {
			// show line
			$('#lineHSK3').css('display', 'flex');
			$('#lineHSK4').css('display', 'flex');
			$('#lineHSK5').css('display', 'flex');
			// shift positioning and show card
			$('#cardHSK3').css('opacity', '1');
			$('#cardHSK3').css('transform', '');
			$('#cardHSK4').css('opacity', '1');
			$('#cardHSK4').css('transform', '');
			$('#cardHSK5').css('opacity', '1');
			$('#cardHSK5').css('transform', '');
			// change + to -
			$('#toggleHSK').text('Minimise Learning Plan');
		} else {
			// show line
			$('#lineHSK3').css('display', 'none');
			$('#lineHSK4').css('display', 'none');
			$('#lineHSK5').css('display', 'none');
			// change + to -
			$('#toggleHSK').text('Expand Learning Plan');
		}

	}

	var region = Intl.DateTimeFormat().resolvedOptions().timeZone;
	if (region.indexOf("Australia") < 0) {
		// convert to USD
		document.getElementById('currencyCode').textContent = 'USD';
		document.getElementById('pracPrice').textContent = '7';
		document.getElementById('addPracPrice').textContent = '7';
		document.getElementById('theoryPracPrice').textContent = '29';
		document.getElementById('theoryPrice').textContent = '29';
	}
</script>