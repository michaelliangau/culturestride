<!-- This is the form that a society director uses to create his/her society login credentials -->
<%- contentFor('body') %>
<body>
    <div class="row">

        <div class="col s12" style="max-width:1000px;padding: 2%;text-align:center">
    <form name="/sohoCreateUser" method="post" id="loginForm">
        <span><h3>Culturestride Create User</h3></span>
        <br>
        <span>First Name</span><br>
        <input type="text" name="firstName" class="validate" placeholder="First Name" required style="width:300px">
        <br>
        <span>Last Name</span><br>
        <input type="text" name="lastName" class="validate" placeholder="Last Name" required style="width:300px">
        <br>
        <span>Email</span><br>
        <input type="text" name="email" class="validate" placeholder="Email" required style="width:300px">
        <br>
        <span>Pay Rate</span><br>
        <input type="text" name="rate" class="validate" placeholder="Rate" required style="width:300px">
        <br>  
        <span>Password</span><br>
        <input type="text" name="password" class="validate" placeholder="Password" required style="width:300px">
        <br>          
        <button class="button button-primary button-block button-shadow" type="submit">Submit</button>
    </form>
    <br>
        </div>
    </div>

</body>